{
  "name": "Palette",
  "version": "1.0",
  "manifest_version": 2,
  "description": "Black your web page.",
  "browser_action": {
    "name": "Change",
    "default_icon": "icon.png",
    "default_title": "Popup",
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs", "http://*/*", "https://*/*","http://api.flickr.com/"
  ],
  "background": {
    "scripts": [
      "script.js",
      "background.js"
    ],
    "persistent": false
  }
}